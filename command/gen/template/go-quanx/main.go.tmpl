package main

import (
	"github.com/go-xuan/quanx"

	{{if .Models}}"{{.App}}/internal/model/entity"
	{{end}}"{{.App}}/internal/router"
)

func main() {
	var engine = quanx.NewEngine()

	// 注册gin路由
	engine.AddGinRouter(router.BindApiRouter)
	{{if .Models}}// 初始化表
	engine.AddTable(
		{{- range $model := .Models}}
		&entity.{{uc $model.Name}}{},
    	{{- end}}
	){{end}}

	// 初始化路由
	engine.RUN()
}